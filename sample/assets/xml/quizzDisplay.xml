<?xml version="1.0" encoding="utf-8" ?>
<Quizz>
    <Background ref="default" src="img/background2.jpg"/>
    <Group ref="default" x="20" y="50" width="2000" height="200" xOffset="20" yOffset="50" resize="false">
        <GroupElement ref="default" width="200" height="80" checkX="5" contentX="25" correctionX="100"
                      background="0x00F000"/>
    </Group>
    <Item ref="good" src="img/ok.png"/>
    <Item ref="CHECK" src="img/checked.png"/>
    <Item ref="CHECKRight" src="img/checked_right.png"/>
    <Item ref="CHECKWrong" src="img/checked_false.png"/>
    <Item ref="UNCHecK" src="img/unchecked.png"/>
    <Text ref="question" x="50" y="300" width="500" height="80"/>
    <Button ref="validate" id="continueButton" type="text" x="400" y="400" scaleX="1" scaleY="1"/>
</Quizz>
